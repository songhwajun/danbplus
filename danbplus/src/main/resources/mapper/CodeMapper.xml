<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.danbplus.util.mybatis.MybatisRepository">
  <select id="getCodeList" resultType="COD_MGT">
    select * from cod_mgt where cod_depth = #{cod_depth};
  </select>

  <select id="getLogin" resultType="LOGIN_MGT">
    select * from login_mgt where usr_id = #{usr_id} and usr_pw = #{usr_pw};
  </select>
  
  <select id="getUserInfo" resultType="USER_INFO">
    select * from user_info where usr_id = #{usr_id};
  </select>
</mapper>